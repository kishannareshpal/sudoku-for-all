{
    "private": true,
    "name": "sudoku-for-all",
    "main": "expo-router/entry",
    "version": "1.0.0",
    "//engines-comment": "Enforces the use of NPM as the package manager and NodeJS LTS.",
    "engines": {
        "node": ">=20",
        "npm": ">=10",
        "yarn": "Please use NPM (from the latest NodeJS LTS) instead of yarn to manage your dependencies. Thank you"
    },
    "scripts": {
        "start": "expo start",
        "start:clear": "expo start --clear",
        "stop": "./scripts/stop",
        "//version:bump-comment": "This is used to update the user facing version of the app befor creating a new store build. Learn more in the Builds section of our README document.",
        "version:bump": "node ./scripts/version-bump",
        "//build:comment": "Learn more about builds in this project by reading the Builds section of our README document.",
        "eas": "npx eas-cli@latest",
        "build:with-eas": "npm run eas -- build",
        "build:with-eas:ios": "npm run build:with-eas -- -p ios",
        "build:with-eas:android": "npm run build:with-eas -- -p android",
        "build:store": "npm run build:with-eas -- -e production --no-wait",
        "build:store:ios": "npm run build:with-eas:ios -- -e production --no-wait",
        "build:store:android": "npm run build:with-eas:android -- -e production --no-wait",
        "build:preview": "npm run build:with-eas -- -e preview",
        "build:preview:ios": "npm run build:with-eas:ios -- -e preview",
        "build:preview:android": "npm run build:with-eas:android -- -e preview",
        "build:preview:ios:simulator": "npm run build:with-eas:ios -- -e preview:simulator",
        "build:development": "npm run build:with-eas -- -e development",
        "build:development:ios": "npm run build:with-eas:ios -- -e development",
        "build:development:android": "npm run build:with-eas:android -- -e development",
        "build:development:ios:simulator": "npm run build:with-eas:ios -- -e development:simulator",
        "build:resign:ios": "npm run eas -- build:resign -p ios",
        "submit:all": "npm run eas -- submit",
        "submit:ios": "npm run eas -- submit -p ios",
        "submit:android": "npm run eas -- submit -p android",
        "device:create": "npm run eas -- device:create",
        "device:rename": "npm run eas -- device:rename",
        "doctor": "npx expo-doctor@latest",
        "expo:gen:typings": "npx expo customize tsconfig.json",
        "pretest": "npm run expo:gen:typings",
        "test": "jest --color",
        "test:watch": "npm run test -- --watch",
        "test:ci": "npm run test --runInBand",
        "precompile": "npm run expo:gen:typings",
        "compile": "tsc --noEmit --skipLibCheck",
        "prelint": "npm run expo:gen:typings",
        "lint": "npx expo lint .",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "husky"
    },
    "dependencies": {
        "@expo/vector-icons": "^14.1.0",
        "@legendapp/state": "^3.0.0-beta.32",
        "@react-navigation/bottom-tabs": "^7.3.10",
        "@react-navigation/elements": "^2.3.8",
        "@react-navigation/native": "^7.1.6",
        "@shopify/react-native-skia": "v2.0.0-next.4",
        "array-fns": "^1.0.0",
        "date-fns": "^4.1.0",
        "expo": "~53.0.9",
        "expo-blur": "~14.1.4",
        "expo-constants": "~17.1.6",
        "expo-dev-client": "~5.2.4",
        "expo-font": "~13.3.1",
        "expo-glass-effect": "^0.1.0",
        "expo-haptics": "~14.1.4",
        "expo-image": "~2.4.0",
        "expo-linking": "~7.1.5",
        "expo-router": "~5.1.5",
        "expo-splash-screen": "~0.30.8",
        "expo-status-bar": "~2.2.3",
        "expo-symbols": "~0.4.4",
        "expo-system-ui": "~5.0.11",
        "expo-web-browser": "~14.2.0",
        "its-fine": "^2.0.0",
        "jotai": "^2.12.5",
        "lucide-react-native": "^0.511.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.5",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-reanimated": "~3.17.4",
        "react-native-safe-area-context": "5.4.0",
        "react-native-screens": "~4.11.1",
        "react-native-svg": "15.11.2",
        "react-native-web": "~0.20.0",
        "react-native-webview": "13.13.5",
        "react-test-renderer": "19.0.0",
        "shallowequal": "^1.1.0",
        "sudoku-gen": "^1.0.2",
        "use-immer": "^0.11.0",
        "zustand": "^5.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@testing-library/react-native": "^13.2.0",
        "@types/jest": "^30.0.0",
        "@types/react": "~19.0.10",
        "@types/shallowequal": "^1.1.5",
        "eslint": "^9.25.0",
        "eslint-config-expo": "~9.2.0",
        "husky": "^9.1.7",
        "jest": "~29.7.0",
        "jest-expo": "~53.0.10",
        "typescript": "~5.8.3"
    },
    "lint-staged": {
        "**/*.{ts,tsx,js}": [
            "npm run lint:fix"
        ]
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
        ]
    }
}
