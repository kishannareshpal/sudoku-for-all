{
  "cli": {
    "version": ">= 5.6.0"
  },
  "build": {
    "ext:base": {
      "node": "20.15.1"
    },
    "ext:development": {
      "extends": "ext:base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "development": {
      "extends": "ext:development",
      "ios": {
        "buildConfiguration": "Debug",
        "image": "latest"
      }
    },
    "development:simulator": {
      "extends": "ext:development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "ext:base",
      "distribution": "internal"
    },
    "preview:simulator": {
      "extends": "ext:base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "ext:base",
      "ios": {
        "buildConfiguration": "Release",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}